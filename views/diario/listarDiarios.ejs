<%- include("../partials/header.ejs"); %>

<body>
    <%- include("../partials/nav.ejs");  %>

    <div class="container">
        <hr>
        <h2 id="titulo"><%= titulo %> </h2>
        <hr>
        
        <% clientes.forEach(cliente => { %> 
            <br>
            <div class="card">
                <div class="card-body">
                    <% { %>
                        <div class="card-header">
                            <h3><%= cliente.placa %></h3>
                        </div>
                        <p> <%= "Horario de entrada: " + cliente.horEntrada + ":" + cliente.minEntrada %></p>
                        <p><%= "Data de entrada: " + cliente.dataEntrada %></p>
                        <a href="/diario/<%= cliente.placa %>">
                            <button class="btn btn-primary">Visualizar</button>
                        </a>
                    <% }%>
                </div>
            </div>
        <% }) %>

    </div>

</body>

<%- include("../partials/footer.ejs"); %>